{
  "name": "app.pinfluencer.io",
  "source": "src/index.html",
  "version": "1.0.0",
  "private": true,
  "devDependencies": {
    "@faker-js/faker": "^7.2.0",
    "@parcel/packager-raw-url": "^2.6.0",
    "@parcel/transformer-webmanifest": "^2.6.0",
    "eslint": "^8.8.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "parcel": "^2.6.0",
    "prettier": "^2.5.1",
    "process": "^0.11.10"
  },
  "dependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^6.3.0"
  },
  "scripts": {
    "start": "parcel",
    "build": "parcel build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "format": "prettier --write \"src/**/*.{js,jsx}\"",
    "format:check": "prettier --check \"src/**/*.{js,jsx}\"",
    "lint": "eslint \"src/**/*.{js,jsx}\" --quiet",
    "deploy": "npm run build; aws --profile pinfluencer s3 sync dist s3://app.pinfluencer.io; aws --profile pinfluencer cloudfront create-invalidation --distribution-id EWGUPH07I0MKD --paths \"/*\" --no-cli-pager",
    "analyze": "source-map-explorer 'dist/static/js/*.js'"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": "> 0.5%, last 2 versions, not dead",
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
