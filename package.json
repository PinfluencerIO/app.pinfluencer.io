{
  "name": "v2",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "start-db": "json-server --watch src/json-server/db.json --port 3001 ",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "format": "prettier --write \"src/**/*.{js,jsx}\"",
    "format:check": "prettier --check \"src/**/*.{js,jsx}\"",
    "lint": "eslint \"src/**/*.{js,jsx}\" --quiet",
    "deploy": "npm run build; aws s3 sync build s3://pinfluencer.link; aws cloudfront create-invalidation --distribution-id EU2RNNCCOPUY0 --paths \"/*\" --no-cli-pager",
    "local-deploy": "npm run build; aws --profile pinfluencer s3 sync build s3://pinfluencer.link; aws  --profile pinfluencer cloudfront create-invalidation --distribution-id EU2RNNCCOPUY0 --paths \"/*\" --no-cli-pager"
  },
  "dependencies": {
    "@emotion/react": "^11.9.3",
    "@emotion/styled": "^11.9.3",
    "@fontsource/comfortaa": "^4.5.9",
    "@mui/icons-material": "^5.10.3",
    "@mui/material": "^5.8.5",
    "aws-amplify": "^4.3.24",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-router": "^6.3.0",
    "react-router-dom": "^6.3.0",
    "react-scripts": "^5.0.1",
    "web-vitals": "^2.1.4",
    "yup": "^0.32.11"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "eslint": "^8.17.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.30.0",
    "eslint-plugin-react-hooks": "^4.5.0",
    "intersection-observer": "^0.12.2",
    "msw": "^0.47.4"
  }
}
